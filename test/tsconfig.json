{
    "compilerOptions": {
        "target": "es5",
        "lib": ["es2015", "dom"],
        "types": ["cypress"],
        "noImplicitAny": false,
        "module": "commonjs",
        "stripInternal": true,
        "alwaysStrict": false,
        "forceConsistentCasingInFileNames": false,
        "noImplicitReturns": false,
        "strict": false,
        "noUnusedLocals": false,
        "jsx": "react",
        "skipLibCheck": true
    },
    "include": ["**/*.ts", "**/*.tsx", "**/*.js"]
}
